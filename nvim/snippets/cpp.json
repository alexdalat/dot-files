{
  "IfElse": {
    "prefix": ["ifel"],
    "body": [
      "if (${1:condition}) {",
      "  ${2:// code}",
      "} else {",
      "  ${3:// code}",
      "}"
    ],
    "description": "If Else Statement"
  },
  "IfElseIf": {
    "prefix": ["ifelif"],
    "body": [
      "if (${1:condition}) {",
      "  ${2:// code}",
      "} else if (${3:condition}) {",
      "  ${4:// code}",
      "} else {",
      "  ${5:// code}",
      "}"
    ],
    "description": "If Else If Statement"
  },
  "TEST": {
    "prefix": ["test"],
    "body": [
      "TEST('test_${1:name}', () => {",
      "  ${2:// code}",
      "});"
    ],
    "description": "TEST Case"
  },
}
